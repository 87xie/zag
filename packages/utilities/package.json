{
  "name": "@ui-machines/utilities",
  "version": "1.0.0",
  "description": "javscript utilities for ui-machines",
  "keywords": [
    "ui-machines",
    "utilities",
    "js-utils",
    "statecharts"
  ],
  "author": "Segun Adebayo <sage@adebayosegun.com>",
  "homepage": "https://github.com/ui-machines/core#readme",
  "license": "MIT",
  "main": "dist/ui-machines-utilities.cjs.js",
  "module": "dist/ui-machines-utilities.esm.js",
  "repository": "https://github.com/ui-machines/core/tree/main/packages/utilities",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/ui-machines-utilities.cjs.js",
      "default": "./dist/ui-machines-utilities.esm.js"
    }
  },
  "preconstruct": {
    "entrypoints": [
      "index.ts",
      "array-utils.ts",
      "assertion-utils.ts",
      "dom-collection-utils.ts",
      "function-utils.ts",
      "number-utils.ts",
      "point-utils.ts",
      "range-utils.ts",
      "rect-utils.ts"
    ]
  },
  "files": [
    "dist",
    "array-utils",
    "assertion-utils",
    "dom-collection-utils",
    "function-utils",
    "number-utils",
    "point-utils",
    "range-utils",
    "rect-utils"
  ],
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/ui-machines/core/issues"
  },
  "scripts": {
    "clean": "rimraf dist"
  }
}
