{
  "name": "@ui-machines/utils",
  "version": "0.1.0",
  "description": "javscript utilities for ui-machines",
  "keywords": [
    "ui-machines",
    "utilities",
    "js-utils",
    "statecharts"
  ],
  "author": "Segun Adebayo <sage@adebayosegun.com>",
  "homepage": "https://github.com/ui-machines/core#readme",
  "license": "MIT",
  "main": "dist/ui-machines-utils.cjs.js",
  "module": "dist/ui-machines-utils.esm.js",
  "repository": "https://github.com/ui-machines/core/tree/main/packages/utilities",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/ui-machines-utils.cjs.js",
      "default": "./dist/ui-machines-utils.esm.js"
    }
  },
  "preconstruct": {
    "entrypoints": [
      "index.ts",
      "array.ts",
      "assertion.ts",
      "env.ts",
      "dom-collection.ts",
      "dom-event.ts",
      "dom-helper.ts",
      "function.ts",
      "number.ts",
      "point.ts",
      "range.ts",
      "rect.ts"
    ]
  },
  "files": [
    "dist",
    "array",
    "assertion",
    "dom-collection",
    "env",
    "dom-event",
    "dom-helper",
    "function",
    "number",
    "point",
    "range",
    "rect"
  ],
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/ui-machines/core/issues"
  },
  "scripts": {
    "clean": "rimraf dist"
  }
}
